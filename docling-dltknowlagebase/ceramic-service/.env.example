# Ceramic Configuration
CERAMIC_URL=http://localhost:5101

# DID Signing Key (generate with: openssl rand -base64 32)
# WARNING: Keep this secret! Never commit to git!
AGENT_DID_SEED_BASE64=your-base64-encoded-seed-here

# IPFS Pinning Service (pinata, infura, or local)
IPFS_PINNING_SERVICE=pinata

# Pinata Configuration (if using Pinata)
PINATA_JWT=your-pinata-jwt-token
# OR use API key/secret instead of JWT
PINATA_API_KEY=your-pinata-api-key
PINATA_API_SECRET=your-pinata-api-secret

# Local IPFS Configuration (if using local IPFS)
IPFS_API_URL=http://localhost:5001

# Server Configuration
PORT=3001
LOG_LEVEL=info
